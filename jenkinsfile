@Library('piper-lib-os') _
node() {
    stage('prepare') {
        checkout scm
        setupCommonPipelineEnvironment script:this
    }


stage('build') {
       mtaBuild script:this
}
stage('deploy') {
    sh 'rm -rf node_modules'
    sh 'pwd;ls -lrt'
    sh 'cf push com.sap.piper.node.hello.world.mtar'
    
}
}
